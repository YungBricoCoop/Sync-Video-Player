{
    "manifest_version": 2,
    "name": "Sync Video Player",
    "version": "0.1",
    "content_scripts": [ {
        "matches": ["<all_urls>"],
        "js": ["jquery-3.6.0.min.js","socket.io.min.js","content.js"]
      } ],
      "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
      },
      "background": {
        "scripts": ["background.js"]
      }
  }