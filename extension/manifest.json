{
    "manifest_version": 3,
    "name": "Sync Video Player",
    "version": "1",
    "content_scripts": [ {
        "matches": ["https://www.youtube.com/*"],
        "js": ["jquery-3.6.0.min.js","socket.io.min.js","content.js"]
      } ],
      "action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
      }
  }